{
  "name": "plantuml",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "statement"
      }
    },
    "statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "preprocessor"
        },
        {
          "type": "SYMBOL",
          "name": "include"
        },
        {
          "type": "SYMBOL",
          "name": "theme"
        },
        {
          "type": "SYMBOL",
          "name": "component"
        },
        {
          "type": "SYMBOL",
          "name": "block"
        },
        {
          "type": "SYMBOL",
          "name": "keyword"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "preprocessor": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "!"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "include": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "!include"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "url",
              "content": {
                "type": "SYMBOL",
                "name": "url"
              }
            },
            {
              "type": "FIELD",
              "name": "filepath",
              "content": {
                "type": "SYMBOL",
                "name": "filepath"
              }
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "<"
                },
                {
                  "type": "FIELD",
                  "name": "library",
                  "content": {
                    "type": "SYMBOL",
                    "name": "non_breakable_identifier"
                  }
                },
                {
                  "type": "STRING",
                  "value": ">"
                }
              ]
            }
          ]
        }
      ]
    },
    "theme": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "!theme"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "from"
        },
        {
          "type": "FIELD",
          "name": "url",
          "content": {
            "type": "SYMBOL",
            "name": "url"
          }
        }
      ]
    },
    "url": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "https?:\\/\\/[^\\s<>\"]+"
      }
    },
    "filepath": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\s<>\"]+"
      }
    },
    "component": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "type",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "FIELD",
          "name": "tag",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "as"
                },
                {
                  "type": "FIELD",
                  "name": "alias",
                  "content": {
                    "type": "SYMBOL",
                    "name": "identifier"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "attribute_list"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "string"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "block": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "identifier"
            },
            {
              "type": "FIELD",
              "name": "component",
              "content": {
                "type": "SYMBOL",
                "name": "component"
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "statement"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "attribute_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "attribute"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "SYMBOL",
                    "name": "attribute"
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "attribute": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "non_breakable_identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_\\/.\\-]+"
    },
    "string": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\""
          },
          {
            "type": "PATTERN",
            "value": "[^\"]*"
          },
          {
            "type": "STRING",
            "value": "\""
          }
        ]
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "'.*"
      }
    },
    "keyword": {
      "type": "PATTERN",
      "value": "@.*"
    },
    "skinparameter": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": ".*"
      }
    },
    "color": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": ".*"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [
    [
      "component",
      "block"
    ]
  ],
  "precedences": [
    [
      {
        "type": "STRING",
        "value": "component"
      },
      {
        "type": "STRING",
        "value": "block"
      }
    ]
  ],
  "externals": [],
  "inline": [],
  "supertypes": []
}
